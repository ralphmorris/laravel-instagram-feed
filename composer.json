{
    "name": "ralphmorris/laravel-instagram",
    "description": "Adds an OAuth Instagram API integration to a Laravel app",
    "keywords": [
        "ralphmorris",
        "laravel-instagram"
    ],
    "homepage": "https://github.com/ralphmorris/laravel-instagram",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ralph Morris",
            "email": "ralph@bubblehubsolutions.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.7.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.7.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "RalphMorris\\LaravelInstagram\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RalphMorris\\LaravelInstagram\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "RalphMorris\\LaravelInstagram\\LaravelInstagramServiceProvider"
            ],
            "aliases": {
                "LaravelInstagram": "RalphMorris\\LaravelInstagram\\LaravelInstagramFacade"
            }
        }
    }
}
